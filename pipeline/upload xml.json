{
	"name": "upload xml",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "XmlSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": {
								"value": "RebateValues*.xml",
								"type": "Expression"
							},
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "XmlReadSettings",
							"validationMode": "none",
							"namespaces": true
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "truncate table [dbo].[RebateValues]",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['ns:RebateValues']['ns:RebateValues'][0]['ns:Rate']"
								},
								"sink": {
									"name": "Rate",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['ns:RebateValues']['ns:RebateValues'][0]['ns:RebateId']"
								},
								"sink": {
									"name": "RebateId",
									"type": "Int64"
								}
							},
							{
								"source": {
									"path": "$['ns:RebateValues']['ns:RebateValues'][0]['ns:RebateValueId']"
								},
								"sink": {
									"name": "RebateValueId",
									"type": "Int64"
								}
							},
							{
								"source": {
									"path": "$['ns:RebateValues']['ns:RebateValues'][0]['ns:ValueRangeFrom']"
								},
								"sink": {
									"name": "ValueRangeFrom",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "$['ns:RebateValues']['ns:RebateValues'][0]['ns:ValueRangeTo']"
								},
								"sink": {
									"name": "ValueRangeTo",
									"type": "Decimal"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "Address",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}